{
  "scripts": {
    "install:api": "cd api && npm i",
    "install:ui": "cd ui && npm i",
    "install": "npm-run-all --parallel --continue-on-error install:*",
    "dev:api": "cd api && npm run dev",
    "dev:ui": "cd ui && npm start",
    "dev": "npm-run-all --parallel --race dev:*"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
